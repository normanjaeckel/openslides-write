# Access point to the event store.
OPENSLIDES_WRITE_SERVICE_EVENT_STORE_URL=http://localhost:9000/

# Timeout: Workers silent for more than this many seconds are killed and
# restarted by Gunicorn.
OPENSLIDES_WRITE_SERVICE_WORKER_TIMEOUT=30

# Debug flag: Set this to 0 to disable debugging, else to 1.
OPENSLIDES_WRITE_SERVICE_DEBUG=1
